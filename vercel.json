{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "headers": [
    {
      "source": "api/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, s-maxage=1"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, s-maxage=1"
        }
      ]
    }
  ]
}
