{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, s-maxage=1"
        }
      ]
    },
    {
      "source": ":id*",
      "has": [
        {
          "type": "query",
          "key": "id"
        }
      ],
      "headers": [
        {
          "key": "cache-control",
          "value": "public, s-maxage=1"
        }
      ]
    }
  ]
}
