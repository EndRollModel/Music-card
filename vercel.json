{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "router/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=1"
        }
      ]
    },
    {
      "source": "views/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=1"
        }
      ]
    }
  ]
}
